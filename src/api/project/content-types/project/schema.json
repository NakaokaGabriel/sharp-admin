{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "projects"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "type": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "site",
        "e-commerce",
        "dashboard",
        "app",
        "system",
        "infrastructure",
        "all"
      ],
      "required": true
    },
    "billing_type": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "json",
      "required": true
    },
    "billing": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "credit",
        "debit"
      ],
      "required": false
    },
    "status": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "active",
        "working",
        "waiting review",
        "inactive",
        "removed"
      ],
      "required": true,
      "default": "active"
    },
    "companies": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::companie.companie",
      "inversedBy": "projects"
    },
    "total_billing": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "decimal",
      "required": true
    },
    "payment": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "credit",
        "debit"
      ]
    }
  }
}
