{
  "kind": "collectionType",
  "collectionName": "user_projects",
  "info": {
    "singularName": "user-project",
    "pluralName": "user-projects",
    "displayName": "user_project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "hosting_status": {
      "type": "enumeration",
      "enum": [
        "Active",
        "Progress",
        "Canceled"
      ],
      "default": "Active",
      "required": true
    },
    "ftp_access": {
      "type": "string",
      "default": "Not Registred",
      "required": true
    },
    "custom_email": {
      "type": "string",
      "default": "Not Registred",
      "required": true
    },
    "expire_at": {
      "type": "date",
      "required": true
    },
    "manager": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
